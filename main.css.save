	/*
	** Logo properties
	*/
	

	#p-logo {
		top: 0;
		left: 0;
		position: absolute; /*needed to use z-index */
		z-index: 3;
		height: 30px;
		width: 12em;
		overflow: inherit;
		background: transparent !important;
	}
	
	#p-logo h3 {
