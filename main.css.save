	/*------------------------------------------------------------------*/
	/*
	** Structural Elements
	*/
	
	/*
	** general portlet styles (elements in the quickbar)
	*/
	.portlet {
			width: 200px;
			vertical-align: top;
			padding: 0;
			background: #313131;
			margin-left: -10px;
			font-weight: bold;
			font-size: 99%;
		/*corrects sidebar*/
			margin-bottom: -35px;
			margin-top: 35px;
	}
	
	/*------------------------------------------------------------------*/
	/* navigation sidebar settings */
	/*this makes the titles blocky*/
	.portlet h3 {
		font-size: 12px;
    line-height: 16px;
    color: #fff;
    padding-left: 20px;
	background: #4f4f4f;
	
		
	}
	
	/* sidebar background */
	.pBody {
		font-size: 95%;
	/*	background-color: #313131;*/
		
		color: #ddd;
		border-collapse: collapse;
		list-style-type:none;
		padding: -0.5em 0.3em;
		/*margin-top: 8%;*/
		padding-bottom: 20px;
		line-height: 26px; /*aligns the sidebar with the TOP TITLE*/
		
	}
	/*------------*/

	/*------------------------------------------------------------------*/
	/* allows .pBody styles to wrap around content added via BaseTemplateAfterPortlet hook */
	.pBody:after {
		content: '';
		clear: both;
		float: left;
		display: block;
	}
	
	.portlet ul { /*this removes the style lists from the nav menu*/
		line-height: 1.6em;
		font-size: 95%;
		list-style: none;
	}
	
	.portlet li {
		padding: 0;
		margin: 0;
		overflow: visible;
		
	}

	.portlet li:hover {
		background: url(/skins/MonoBook/pics/menu-arrow.png)  center right no-repeat transparent;
		background-position-x: -10px;	
		overflow: visible;
	}
	
